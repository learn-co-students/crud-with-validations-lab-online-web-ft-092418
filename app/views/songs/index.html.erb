<body>
    
<table>

    <thead>
        <th>Title</th>
        <th>Artist</th>
        <th>Released?</th>
        <th>Year Released</th>
        <th>Genre</th>
    </thead>

    <tbody>
    <% @songs.each do |song| %>
        <tr>
            <td><%= song.title %></td>        
                <td><%= song.artist_name %> </td>
                <td><% if song.released %>
                    Released
                <% else %>
                    Unreleased
                <% end %> </td>
                <td> <%= song.release_year if song.released %> </td>
                <td> <%= song.genre %> </td>
        </tr>
    <% end %>
    </tbody>
</table>
</body>